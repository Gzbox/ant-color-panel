{
  "name": "ant-color-panel",
  "version": "1.3.0",
  "description": "Ant Design Color Panel",
  "keywords": [
    "react",
    "antd",
    "ant-design"
  ],
  "main": "./lib/index",
  "module": "./es/index",
  "files": [
    "es",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Gzbox/ant-color-panel.git"
  },
  "bugs": {
    "url": "https://github.com/Gzbox/ant-color-panel/issues"
  },
  "sideEffects": [
    "*.css"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "PREVIEW=1 dumi build",
    "docs:deploy": "gh-pages -d .doc",
    "compile": "father build",
    "gh-pages": "npm run docs:build && npm run docs:deploy",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1",
    "@ctrl/tinycolor": "^3.6.0",
    "antd": "^5.4.2",
    "classnames": "^2.3.1",
    "rc-util": "^5.21.5",
    "react-colorful": "^5.5.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/tinycolor2": "^1.4.3",
    "@umijs/fabric": "^3.0.0",
    "cross-env": "^7.0.2",
    "dumi": "^2.0.9",
    "eslint": "^8.41.0",
    "father": "^4",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "np": "^7.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "cnpm": {
    "mode": "npm"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,md,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
